{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/src/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.7.0",
    "graphql": "15.8.0",
    "graphql-request": "4.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.10",
    "@graphql-codegen/typescript-document-nodes": "2.2.10",
    "@graphql-codegen/typescript-graphql-request": "4.4.7",
    "@graphql-codegen/typescript-operations": "2.3.7",
    "@graphql-codegen/typescript-resolvers": "2.6.3",
    "@octokit/graphql-schema": "10.73.0",
    "@types/node": "16.11.33",
    "@typescript-eslint/parser": "5.22.0",
    "@vercel/ncc": "0.33.4",
    "eslint": "8.15.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "js-yaml": "4.1.0",
    "prettier": "2.6.2",
    "ts-jest": "27.1.4",
    "typescript": "4.6.4"
  }
}